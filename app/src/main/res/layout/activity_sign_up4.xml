<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUpActivity4">

    <TextView
        android:id="@+id/preferenciasBusquedaText"
        android:layout_width="234dp"
        android:layout_height="40dp"
        android:ems="@integer/titleSize"
        android:text="@string/menu_preferencias"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.068" />

    <SeekBar
        android:id="@+id/seekBarEdadMinimaRegistro4"
        android:layout_width="213dp"
        android:layout_height="40dp"
        android:max="32"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sobreMiText"
        app:layout_constraintVertical_bias="0.025" />

    <TextView
        android:id="@+id/sobreMiText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edad mínima:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.141"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintVertical_bias="0.121" />

    <SeekBar
        android:id="@+id/seekBarEdadMaximaRegistro4"
        android:layout_width="213dp"
        android:layout_height="40dp"
        android:max="32"
        android:progress="100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.044" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edad máxima:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBarEdadMinimaRegistro4"
        app:layout_constraintVertical_bias="0.084" />

    <TextView
        android:id="@+id/edadMinima"
        android:layout_width="56dp"
        android:layout_height="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.232"
        app:layout_constraintStart_toEndOf="@+id/seekBarEdadMinimaRegistro4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.324" />

    <TextView
        android:id="@+id/edadMaxima"
        android:layout_width="56dp"
        android:layout_height="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toEndOf="@+id/seekBarEdadMaximaRegistro4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.485" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="81dp"
        android:layout_height="21dp"
        android:text="Género:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.136"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/preferenciasBusquedaText"
        app:layout_constraintVertical_bias="0.016" />

    <CheckBox
        android:id="@+id/checkHombreRegistro4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Masculino"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.155"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/preferenciasBusquedaText"
        app:layout_constraintVertical_bias="0.062" />

    <CheckBox
        android:id="@+id/checkMujerRegistro4"
        android:layout_width="101dp"
        android:layout_height="48dp"
        android:text="Femenino"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toEndOf="@+id/checkHombreRegistro4"
        app:layout_constraintTop_toBottomOf="@+id/preferenciasBusquedaText"
        app:layout_constraintVertical_bias="0.062" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="125dp"
        android:layout_height="20dp"
        android:text="Facultad y carrera:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBarEdadMaximaRegistro4"
        app:layout_constraintVertical_bias="0.086" />

    <Spinner
        android:id="@+id/spinnerFacultadesRegistro4"
        android:layout_width="320dp"
        android:layout_height="37dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.071" />

    <Spinner
        android:id="@+id/spinnerCarrerasRegistro4"
        android:layout_width="320dp"
        android:layout_height="37dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerFacultadesRegistro4"
        app:layout_constraintVertical_bias="0.072" />

    <CheckBox
        android:id="@+id/checkOtroRegistro4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Otro"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toEndOf="@+id/checkMujerRegistro4"
        app:layout_constraintTop_toBottomOf="@+id/preferenciasBusquedaText"
        app:layout_constraintVertical_bias="0.062" />

    <Button
        android:id="@+id/btnSiguienteRegistro4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>